{
    "name": "paysera/lib-database-init-bundle",
    "description": "Initializes database for your project",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Paysera\\Bundle\\DatabaseInitBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paysera\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4||^8.0",
        "paysera/lib-dependency-injection": "^1.0",
        "doctrine/dbal": "^2.5",
        "doctrine/data-fixtures": "^1.0",
        "doctrine/annotations": "^1.14",
        "symfony/finder": "^5.0",
        "symfony/console": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/doctrine-bridge": "^5.0"
    },
    "require-dev": {
        "symfony/symfony": "^5.0",
        "phpunit/phpunit": "^9.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "symfony/monolog-bundle": "^3.1",
        "doctrine/doctrine-migrations-bundle": "^3.2"
    }
}
